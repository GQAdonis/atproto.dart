{
  "lexicon": 1,
  "id": "app.bsky.bookmark.get",
  "defs": {
    "main": {
      "type": "query",
      "description": "Get a bookmark by URI",
      "parameters": {
        "type": "params",
        "properties": {
          "uri": {
            "type": "string",
            "format": "at-uri"
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["bookmark"],
          "properties": {
            "bookmark": {
              "type": "ref",
              "ref": "app.bsky.bookmark.defs#bookmark"
            }
          }
        }
      }
    }
  }
}
